  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#92a09e"
        }
      },
      {
        "name": "String",
        "scope": ["string", "punctuation.definition.string"],
        "settings": {
          "foreground": "#2aa28a"
        }
      },
      {
        "name": "Number",
        "scope": ["constant.numeric"],
        "settings": {
          "foreground": "#dc2e3f"
        }
      },
      {
        "name": "Built-in constant",
        "scope": ["constant.language"],
        "settings": {
          "foreground": "#dc2e3f"
        }
      },
      {
        "name": "User-defined constant",
        "scope": ["constant.character", "constant.other"],
        "settings": {
          "foreground": "#dc2e3f"
        }
      },
      {
        "name": "Variable",
        "scope": ["variable"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Keyword",
        "scope": ["keyword"],
        "settings": {
          "foreground": "#969900"
        }
      },
      {
        "name": "Storage",
        "scope": ["storage"],
        "settings": {
          "foreground": "#969900"
        }
      },
      {
        "name": "Storage type",
        "scope": ["storage.type"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Class name",
        "scope": ["entity.name.class"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Function name",
        "scope": ["entity.name.function"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Function argument",
        "scope": ["variable.parameter"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#ca3716"
        }
      },
      {
        "name": "Tag name",
        "scope": ["entity.name.tag"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Tag attribute",
        "scope": ["entity.other.attribute-name"],
        "settings": {
          "foreground": "#969900"
        }
      },
      {
        "name": "Library function",
        "scope": ["support.function"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Library constant",
        "scope": ["support.constant"],
        "settings": {
          "foreground": "#dc2e3f"
        }
      },
      {
        "name": "Library class/type",
        "scope": ["support.type", "support.class"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Library variable",
        "scope": ["support.other.variable"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Invalid",
        "scope": ["invalid"],
        "settings": {
          "background": "#dc2e3f",
          "foreground": "#fdf3e2"
        }
      },
      {
        "name": "Invalid deprecated",
        "scope": ["invalid.deprecated"],
        "settings": {
          "background": "#ca3716",
          "foreground": "#fdf3e2"
        }
      },
      {
        "name": "JSON String",
        "scope": ["meta.structure.dictionary.json string.quoted.double.json"],
        "settings": {
          "foreground": "#b37100"
        }
      },
      {
        "name": "diff.header",
        "scope": ["meta.diff", "meta.diff.header"],
        "settings": {
          "foreground": "#92a09e"
        }
      },
      {
        "name": "diff.deleted",
        "scope": ["markup.deleted"],
        "settings": {
          "foreground": "#dc2e3f"
        }
      },
      {
        "name": "diff.inserted",
        "scope": ["markup.inserted"],
        "settings": {
          "foreground": "#969900"
        }
      },
      {
        "name": "diff.changed",
        "scope": ["markup.changed"],
        "settings": {
          "foreground": "#b37100"
        }
      },
      {
        "name": "CSS Classes",
        "scope": ["entity.other.attribute-name.class.css"],
        "settings": {
          "foreground": "#b37100"
        }
      },
      {
        "name": "CSS IDs",
        "scope": ["entity.other.attribute-name.id.css"],
        "settings": {
          "foreground": "#b37100"
        }
      },
      {
        "name": "CSS Properties",
        "scope": ["support.type.property-name.css"],
        "settings": {
          "foreground": "#279fd3"
        }
      },
      {
        "name": "CSS Property Values",
        "scope": [
          "support.constant.property-value.css",
          "constant.numeric.css"
        ],
        "settings": {
          "foreground": "#2aa28a"
        }
      },
      {
        "name": "Markdown Headings",
        "scope": ["markup.heading"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#279fd3"
        }
      },
      {
        "name": "Markdown Links",
        "scope": ["markup.underline.link"],
        "settings": {
          "foreground": "#2aa28a"
        }
      },
      {
        "name": "Markdown Bold",
        "scope": ["markup.bold"],
        "settings": {
          "fontStyle": "bold",
          "foreground": "#ca3716"
        }
      },
      {
        "name": "Markdown Italic",
        "scope": ["markup.italic"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#ca3716"
        }
      }
    ]
  },
