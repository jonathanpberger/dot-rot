// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+cmd+a",
    "command": "editor.emmet.action.incrementNumberByOne"
  },
  {
    "key": "ctrl+alt+cmd+7",
    "command": "runCommands",
    "args": {
      "commands": [
        "markdown-table-formatter.toggleDebugDecorations",
        "markdown-table-formatter.format",
        "markdownExtended.formattingTables",
        "markdown.extension.formattingTable",
        "markdown.extension.onTableFormat",
        "markdown-all-in-one.tableFormat"
      ]
    },
    "when": "editorTextFocus && editorLangId == markdown"
  },
  {
    "key": "ctrl+alt+cmd+1",
    "command": "outline.collapse"
  },
  {
    "key": "ctrl+alt+cmd+2",
    "command": "outline.followCursor"
  },
  {
    "key": "ctrl+alt+cmd+8",
    "when": "editorTextFocus",
    "command": "moveby.regex",
    "args": {
      "regex": "üçÖ Todo Today",
      "properties": ["prev", "start"]
    }
  },
  {
    "key": "ctrl+alt+cmd+9",
    "when": "editorTextFocus",
    "command": "moveby.regex",
    "args": {
      "regex": "Week.init",
      "properties": ["prev", "start"]
    }
  },
  {
    "key": "ctrl+alt+cmd+3",
    "command": "workbench.action.evenEditorWidths"
  },
  {
    "key": "ctrl+alt+cmd+4",
    "command": "runCommands",
    "args": {
      "commands": [
        "workbench.action.closeSidebar",
        "workbench.action.minimizeOtherEditors"
      ]
    }
  },
  {
    "key": "ctrl+alt+cmd+5",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+alt+cmd+6",
    "command": "markdown-table-formatter.toggleDebugDecorations",
    "when": "editorTextFocus && editorLangId == markdown"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "shift+cmd+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "cmd+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "alt+cmd+f2",
    "command": "editor.action.changeAll",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "cmd+f2",
    "command": "-editor.action.changeAll",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+cmd+f3",
    "command": "editor.action.nextSelectionMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "cmd+f3",
    "command": "-editor.action.nextSelectionMatchFindAction",
    "when": "editorFocus"
  },
  {
    "key": "alt+cmd+d",
    "command": "foam-vscode.open-daily-note"
  },
  {
    "key": "alt+d",
    "command": "-foam-vscode.open-daily-note"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
},
{
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
        "text": "\\\r\n"
    },
    "when": "terminalFocus"
}
]